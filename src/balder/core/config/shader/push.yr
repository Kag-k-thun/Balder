in push;

pub record PushConstantInfo {
    pub let mut offset : u32 = 0;    
    pub let mut size   : u32 = 0;
    
    pub self () {}
}

pub record PushConstantConfig {    
    pub let dmut set : [[c8] => PushConstantInfo] = copy [];    
    pub let mut size : u32                        = 0;

    pub self () {}
}
